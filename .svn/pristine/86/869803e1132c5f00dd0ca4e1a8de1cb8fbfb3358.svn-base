	min-width: calc(4em + 15px);
}
.chucangbtm{
	position: fixed;
	bottom: 0px;
	width: 100%;
	background-color: #FFFFFF;
	z-index: 2;
}
.lastState{
	margin-top: 30px;
	/* bottom: 0px; */
	display: flex;
	flex-direction:column;
	/* padding: 20px; */
	/* justify-content:center; */
	text-align: center;
}
</style>
